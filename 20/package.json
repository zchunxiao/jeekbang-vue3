{
  "name": "my",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:components  && npm run build:css",
    "build:components": "vite-node ./scripts/build-module.ts",
    "build:dts": "vite-node ./scripts/build-dts.ts",
    "build:css": "vite-node ./scripts/build-css.ts",
    "build:work": "npm run build:work-front && npm run build:work-server",
    "build:work-front": "vite-node ./scripts/build-work-front.ts",
    "build:work-server": "vite-node ./scripts/build-work-server.ts",
    "dev:components": "vite-node ./scripts/dev-components.ts",
    "dev:business": "vite-node ./scripts/dev-business.ts",
    "dev:work-server": "cross-env NODE_ENV=development nodemon --watch 'packages/work-server/src/**/*' --watch 'packages/work-server/node_modules/**/*' --exec 'ts-node --project ./tsconfig.node.json' packages/work-server/src/index.ts",
    "dev:work-front": "vite-node ./scripts/dev-work-front.ts",
    "postinstall": "npm run build",
    "test": "vitest",
    "test:update": "vitest --update",
    "coverage": "vitest run --coverage",
    "clear": "npm run clear:deps && npm run clear:dist",
    "clear:deps": "rm -rf ./node_modules && rm -rf packages/*/node_modules/",
    "clear:dist": "rm -rf ./dist && rm -rf packages/*/dist/",
    "db:init": "vite-node ./packages/init-database/src/index.ts",
    "db:query": "vite-node ./packages/init-database/src/select.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.4",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@tsconfig/node18": "^1.0.1",
    "@types/koa": "^2.13.5",
    "@types/less": "^3.0.3",
    "@types/node": "^18.11.5",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vitest/coverage-c8": "^0.24.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-vue": "^9.6.0",
    "fast-glob": "^3.2.12",
    "jsdom": "^20.0.2",
    "koa": "^2.13.4",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "rollup-plugin-esbuild": "^4.10.1",
    "terser": "^5.15.1",
    "ts-loader": "^9.4.1",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "unplugin-vue-macros": "^0.14.0",
    "vite": "^3.1.8",
    "vite-node": "^0.24.3",
    "vitest": "^0.24.5",
    "vue": "^3.2.41",
    "vue-loader": "^17.0.1",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "vue-router": "4"
  }
}
